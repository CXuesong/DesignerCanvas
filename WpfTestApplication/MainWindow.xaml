<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTestApplication"
        xmlns:DesignerCanvas="clr-namespace:Undefined.DesignerCanvas;assembly=DesignerCanvas" x:Class="WpfTestApplication.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="4*"/>
        </Grid.RowDefinitions>
        <Border BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}" BorderThickness="1" Background="#FFD4EDFF">
            <DesignerCanvas:DesignerCanvas x:Name="dcvs">
                <DesignerCanvas:DesignerCanvasItem Margin="267,93,137,76"/>
            </DesignerCanvas:DesignerCanvas>
        </Border>
        <Button x:Name="TestButton" Content="Button" HorizontalAlignment="Left" Margin="46,15,0,0" VerticalAlignment="Top" Width="75" Grid.Row="1" Click="TestButton_Click" Command="{Binding test}"/>
    </Grid>
</Window>
